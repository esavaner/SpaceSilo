{
  "jsc": {
    "target": "es2024",
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "keepClassNames": true,
    "baseUrl": "./src"
  },
  "module": {
    "type": "es6"
  }
}
